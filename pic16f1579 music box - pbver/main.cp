 break;
 case 2:
 c3CapDischargeCount = 0;
 TRISC.TRISC2 = 0;
 PWM3PR = tuningWords[songDataInput[songIndex+1]];
 PWM3DC = tuningWords[songDataInput[songIndex+1]] >> 1;
 PWM3LDCON.PWM3LD = 1;
 PWM3CON.PWM3EN = 1;
 break;
 default:
 break;
 }
 songIndex += 2;

 }

 else if(opcode == 0xf0) {





 if(songBankCount > 4)
 songBankCount = 0;
 else
 songBankCount++;

 songDataInput = songAddr[songBankCount];

 c1CapDischargeCount = 0;
 c2CapDischargeCount = 0;
 c3CapDischargeCount = 0;

